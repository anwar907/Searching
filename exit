[36m@@ -0,0 +1,35 @@[m
[32m+[m[32mpackage com.mycompany.searching;[m
[32m+[m
[32m+[m[32mpublic class BinarySearch {[m
[32m+[m
[32m+[m[32m    public static int binarySearch(int arr[], int l, int r, int x) {[m
[32m+[m[32m        if (r >= 1) {[m
[32m+[m[32m            int mid = l + (r - l) / 2;[m
[32m+[m
[32m+[m[32m            if (arr[mid] == x) {[m
[32m+[m[32m                return mid;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (arr[mid] > x) {[m
[32m+[m[32m                return binarySearch(arr, l, mid - 1, x);[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            return binarySearch(arr, mid + 1, r, x);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return -1;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static void main(String args[]) {[m
[32m+[m[32m        BinarySearch in = new BinarySearch();[m
[32m+[m[32m        int arr[] = {1, 2, 3, 4, 5, 6, 7, 8};[m
[32m+[m[32m        int n = arr.length;[m
[32m+[m[32m        int x = 7;[m
[32m+[m[32m        int result = BinarySearch.binarySearch(arr, 0, n - 1, x);[m
[32m+[m[32m        if (result == -1) {[m
[32m+[m[32m            System.out.println("Elemen not present");[m
[32m+[m[32m        } else {[m
[32m+[m[32m            System.out.println("Elemen found at index = " + result);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
