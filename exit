[32m+[m[32mpackage com.mycompany.searching;[m
[32m+[m
[32m+[m[32mpublic class LinearSearch {[m
[32m+[m
[32m+[m[32m    public static int linearSearch(int[] inputArray, int key) {[m
[32m+[m[32m        for (int i = 0; i < inputArray.length; i++) {[m
[32m+[m[32m            if (inputArray[i] == key) {[m
[32m+[m[32m                return i;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        return -1;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static void main(String args[]) {[m
[32m+[m[32m        int[] arr1 = {5, 9, 10, 2, 90, 4};[m
[32m+[m
[32m+[m[32m        int key = 2;[m
[32m+[m[32m        int result = linearSearch(arr1, key);[m
[32m+[m[32m        System.out.println((result != -1) ? "Required key: " + key + "